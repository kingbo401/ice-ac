<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="MysqlContext" targetRuntime="MyBatis3" defaultModelType="flat">
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>

        <commentGenerator>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/ice_acl"
                        userId="root"
                        password="root">
        </jdbcConnection>

        <javaModelGenerator targetPackage="kingbo401.iceacl.common.model.po.test" targetProject="src/main/java">
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mybatis/mapper/test"  targetProject="src/main/resources"/>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.kingbo401.iceacl.dao.test"  targetProject="src/main/java"/>

        <table tableName="ice_app" domainObjectName="AppPO" mapperName="AppDAO">
            <generatedKey column="id" sqlStatement="Mysql"/>
        </table>
    </context>
</generatorConfiguration>